{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/whitepaper.pdf",
      "has": [{ "type": "host", "value": "https://urbz-token.vercel.app/" }],
      "permanent": true
    }
  ]
}
